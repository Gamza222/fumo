    defaultOptions: {
        queries: {
            // Enable Suspense mode for React Query v5
            throwOnError: true,
            retry: function (failureCount, error) {
                // Don't retry on 4xx errors
                if (error && typeof error === 'object' && 'status' in error) {
                    var status = error.status;
                    if (status >= 400 && status < 500)
                        return false;
