{
  "name": "fumo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "ts-node --project tsconfig.scripts.json scripts/generate-build-info.ts && next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --config config/eslint/.eslintrc.js",
    "lint:fix": "eslint . --ext .ts,.tsx --fix --config config/eslint/.eslintrc.js",
    "type-check": "tsc --noEmit",
    "test": "jest --config ./config/jest/jest.config.ts",
    "test:watch": "jest --config ./config/jest/jest.config.ts --watch",
    "test:ci": "jest --config ./config/jest/jest.config.ts --coverage",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --config config/prettier/.prettierrc.js",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\" --config config/prettier/.prettierrc.js",
    "version:current": "ts-node --project tsconfig.scripts.json scripts/version.ts current",
    "version:info": "ts-node --project tsconfig.scripts.json scripts/version.ts info",
    "version:display": "ts-node --project tsconfig.scripts.json scripts/version.ts display",
    "version:env": "ts-node --project tsconfig.scripts.json scripts/version.ts env",
    "version:ci": "ts-node --project tsconfig.scripts.json scripts/version.ts ci",
    "version:validate": "ts-node --project tsconfig.scripts.json scripts/version.ts validate",
    "version:buildinfo": "ts-node --project tsconfig.scripts.json scripts/version.ts buildinfo",
    "version:bump:patch": "ts-node --project tsconfig.scripts.json scripts/version.ts bump:patch",
    "version:bump:minor": "ts-node --project tsconfig.scripts.json scripts/version.ts bump:minor",
    "version:bump:major": "ts-node --project tsconfig.scripts.json scripts/version.ts bump:major",
    "version:bump:prerelease": "ts-node --project tsconfig.scripts.json scripts/version.ts bump:prerelease",
    "version:release": "ts-node --project tsconfig.scripts.json scripts/version.ts release",
    "version:help": "ts-node --project tsconfig.scripts.json scripts/version.ts help",
    "build:info": "ts-node --project tsconfig.scripts.json scripts/generate-build-info.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "analyze": "ANALYZE=true npm run build",
    "analyze:server": "BUNDLE_ANALYZE=server npm run build",
    "analyze:browser": "BUNDLE_ANALYZE=browser npm run build",
    "generate:component": "node scripts/generate-component.js",
    "validate:ci": "ts-node --project tsconfig.scripts.json scripts/validate-ci.ts",
    "fix:ci": "ts-node --project tsconfig.scripts.json scripts/fix-ci-issues.ts"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.0.0",
    "@jest/globals": "^29.7.0",
    "@next/bundle-analyzer": "^15.5.2",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/nextjs": "^8.6.14",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.19",
    "@types/react": "^18.2.57",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-transform-stub": "^2.0.0",
    "node-mocks-http": "^1.17.2",
    "playwright": "^1.53.0",
    "prettier": "^3.2.5",
    "sass": "^1.89.2",
    "storybook": "^8.6.14",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "web-vitals": "^5.1.0",
    "whatwg-fetch": "^3.6.20"
  },
  "dependencies": {
    "@apollo/client": "^4.0.1",
    "@sentry/nextjs": "^9.40.0",
    "@sentry/react": "^9.40.0",
    "@tanstack/react-query": "^5.83.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "bcryptjs": "^3.0.2",
    "graphql": "^16.11.0",
    "immer": "^10.1.1",
    "jsonwebtoken": "^9.0.2",
    "next": "^15.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^6.0.0",
    "uuid": "^11.1.0",
    "zod": "^4.1.1",
    "zustand": "^5.0.8"
  }
}
